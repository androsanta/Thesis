{
  "service": "rln-project-offline",
  "provider": {
    "name": "aws",
    "runtime": "nodejs12.x",
    "iamRoleStatements": [
      {
        "Effect": "Allow",
        "Resource": "*",
        "Action": [
          "lambda:InvokeFunction"
        ]
      }
    ]
  },
  "plugins": [
    "serverless-offline",
    "serverless-plugin-warmup",
    "serverless-mongo-proxy"
  ],
  "functions": {
    "postUsers": {
      "handler": "dist/exporter.postUsers",
      "events": [
        {
          "http": {
            "path": "users",
            "method": "post",
            "cors": true,
            "authorizer": null
          }
        }
      ],
      "warmup": {
        "enabled": true
      }
    },
    "clean": {
      "handler": "dist/schedulesExporter.clean",
      "warmup": {
        "enabled": false
      },
      "events": [
        {
          "schedule": {
            "name": "clean",
            "description": "",
            "enabled": true
          }
        }
      ]
    }
  },
  "custom": {
    "warmup": {
      "prewarm": true,
      "enabled": true
    }
  }
}